{{ form_start(form) }}
<div class="jumbotron">
    <div class="row">
        <div class="col-md-4">{{ form_row(form.title) }}</div>
        <div class="col-md-4">{{ form_row(form.surface) }}</div>
        <div class="col-md-4">{{ form_row(form.price) }}</div>
    </div>
    <div class="row">
        <div class="col-md-2">{{ form_row(form.rooms) }}</div>
        <div class="col-md-2">{{ form_row(form.bedrooms) }}</div>
        <div class="col-md-2">{{ form_row(form.floor) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.adress) }}</div>
        <div class="col-md-3">{{ form_row(form.city) }}</div>
        <div class="col-md-3">{{ form_row(form.code_postal) }}</div>
    </div>
    <div class="row">
        <div class="col-md-8">{{ form_row(form.description) }}</div>
    </div>
    <div class="row">{{ form_row(form.sold) }}</div>
    <div class="row">
        <button class="btn btn-block btn-primary">{{ button|default('Cr√©er') }}</button>
    </div>
</div>
{{ form_end(form) }}